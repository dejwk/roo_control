{
    "name": "roo_control",
    "description": "Arduino relays, port extenders, debouncing, retries: common abstractions and implementations.",
    "keywords": [
        "relay",
        "PCF8574",
        "switch"
    ],
    "authors": [
        {
            "name": "Dejwk",
            "email": "dejwk.oss@gmail.com",
            "maintainer": true
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/dejwk/roo_control"
    },
    "version": "1.0.2",
    "homepage": "https://github.com/dejwk/roo_control",
    "frameworks": [
        "Arduino"
    ],
    "build": {
        "srcDir": "src",
        "srcFilter": "+<*> -<examples> -<benchmarks> -<doc> -<test>"
    },
    "examples": [
        "examples/**/*.ino"
    ],
    "dependencies": {
        "Wire": ">0",
        "dejwk/roo_logging": ">=1.0.4",
        "dejwk/roo_collections": ">=1.1.6",
        "dejwk/roo_prefs": ">=1.0.3",
        "dejwk/roo_scheduler": ">=1.2.0",
        "dejwk/roo_temperature": ">0",
        "dejwk/roo_time": ">0",
        "nanopb/Nanopb": ">=0.4.91"
    }
}
